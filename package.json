{
  "name": "learn-english",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href /learn/ --deploy-url /learn/ --output-path ./docs/",
    "build:assets": "ng build --prod --base-href . --output-path ./cordova/www/",
    "build:assets:dev": "ng build --base-href . --output-path ./cordova/www/",
    "build:cordova": "cd cordova && cordova build \"windows\"",
    "emulate:browser": "npm run build:assets:dev && cd cordova && cordova emulate \"browser\"",
    "emulate:android": "npm run build:assets:dev && cd cordova && cordova emulate \"android\"",
    "build:app": "npm run build:assets && npm run build:cordova",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:windows": "WinAppDeployCmd install -file \"./cordova/platforms/windows/AppPackages/CordovaApp.Windows10_1.0.1.0_anycpu_debug_Test/CordovaApp.Windows10_1.0.1.0_anycpu_debug.appx\" -ip \"192.168.2.105\"",
    "deploy:app:windows": "npm run build:app && npm run deploy:windows"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngrx/effects": "^8.0.0",
    "@ngrx/entity": "^8.0.0",
    "@ngrx/router-store": "^8.0.0",
    "@ngrx/schematics": "^8.0.0",
    "@ngrx/store": "^8.0.0",
    "@ngrx/store-devtools": "^8.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
